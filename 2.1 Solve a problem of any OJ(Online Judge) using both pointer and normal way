//user given array of integers, find the sum of all elements
#include <stdio.h>

int findSum(int arr[], int size) {
    int sum = 0;
    for (int i = 0; i < size; i++) {
        sum += arr[i];
    }
    return sum;
}

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(arr[0]);
    int result = findSum(arr, size);
    printf("Sum using normal approach: %d\n", result);
    return 0;
}
